{
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "buildCommand": "npm run build",
  "devCommand": "npm run start",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/(.*)", "dest": "/api/$1" }, // Ensure API routes are handled
    { "src": "/(.*)", "status": 404, "dest": "/404" }
  ]
}
